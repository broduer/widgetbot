    "uuid": "^3.2.1",
    "winston": "2.4.3",
    "winston-console-formatter": "^1.0.0-beta.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/scripts/jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|js)"
    ]
  },
  "devDependencies": {
    "@types/errorhandler": "^0.0.32",
    "@types/jest": "^22.2.3",
    "awesome-typescript-loader": "^5.0.0",
    "chalk": "^2.4.1",
