    "@types/memoizee": "^0.4.2",
    "@types/nedb": "^1.8.5",
    "@types/raven": "^2.5.1",
    "@types/react-notification-system": "^0.2.37",
    "@types/request": "^2.47.0",
    "@types/socket.io": "^1.4.33",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.3",
    "@types/winston": "2.3.9",
    "@widgetbot/nedb": "^3.0.2",
    "country-emoji": "^1.2.0",
    "discord.js": "^11.3.2",
    "doasync": "^2.0.0",
    "expect": "^23.1.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-interceptor": "^1.2.0",
    "express-rate-limit": "^2.11.0",
    "express-winston": "^2.5.1",
    "function-pipe": "^1.2.1",
    "geoip-lite": "^1.2.1",
    "graphql": "^0.13.2",
    "html-template-tag": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.10",
    "memoizee": "^0.4.12",
    "node-yaml": "^3.1.1",
    "on-build-webpack": "^0.1.0",
    "os-name": "^2.0.1",
    "pmx": "^1.6.4",
    "pretty-bytes": "^5.0.0",
    "pretty-ms": "^3.2.0",
    "raven": "^2.6.2",
    "request": "^2.87.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "source-map-support": "^0.5.6",
